<%= simple_form_for(@profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
   
    <%= f.input :name %>
    <%= f.input :profile_pic, as: :file %>
    <%= f.input :bio %>
    <%= f.input :phone %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
